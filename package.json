{
  "name": "nestjs-starter-kit",
  "version": "0.0.2",
  "description": "Nest.js with MongoDB, Express, Clustering, Swagger, Pino, Exception handling",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piyush-kacha/nestjs-starter-kit.git"
  },
  "author": "Piyush Kacha <piyushkacha75@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piyush-kacha/nestjs-starter-kit/issues"
  },
  "homepage": "https://github.com/piyush-kacha/nestjs-starter-kit#readme",
  "dependencies": {
    "@nestjs/common": "^10.0.4",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.4",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.4",
    "@nestjs/swagger": "^7.1.8",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.4.3",
    "nestjs-pino": "^3.4.0",
    "pino": "^8.15.0",
    "pino-http": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1"
   },
   "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.4",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "18.16.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.2",
    "lint-staged": "^13.3.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
   }
}
